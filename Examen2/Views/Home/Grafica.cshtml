@{
    Layout = "";
}

<style>
    .connectedSortable {
  min-height: 100px;
}
</style>

<div ng-show="MostrarVista == 1">
    <section class="content-header">
        <h1> Agenda de seguimiento </h1>
        <div class="externo loaderGrafica" ng-show="mostrarLoader">
            <div class="loader interno"></div>
        </div>
        <ol class="breadcrumb">
            <li><i class="fa fa-dashboard"></i> Capital Humano </li>
            <li class="active"> Agenda de seguimiento </li>
        </ol>
    </section>
    <section class="content">
        <div class="box box-primary">
            <div class="box-body">
            </div>
        </div>

        <div class="box">
            <div class="box-body with-border">
                <h3 class="box-title">
                    Evaluaciones
                </h3>

                <div class="box-tools pull-right">
                    <button type="button" ng-click="VolverInicio()" class="btn btn-primary btn-tool" title="Regresar a la página anterior"><i class="fa fa-backward"></i> Regresar </button>
                </div>

                <div class="row">

                    <div class="box-body chart-responsive col-lg-12">
                        <div class="chart tab-pane active" id="bar-chart-calificaciones" style="display:block; height: 500px;"></div>
                    </div>
                </div>


            </div>

        </div>


